{
  "name": "windows-interactive-notifications",
  "version": "0.2.2",
  "description": "Interactive notifications for Windows, from Node",
  "main": "lib/index.js",
  "scripts": {
    "postinstall": "node ./scripts/postinstall.js",
    "preversion": "npm run build && npm run copydlls && npm run commitdlls",
    "copydlls": "xcopy .\\Release .\\dll\\ia32 /y && xcopy .\\x64\\Release .\\dll\\x64 /y /e /v",
    "commitdlls": "git add .\\dll\\ && git commit -m \":package: Update dlls\" || exit 0;",
    "build64": " msbuild .\\InteractiveNotifications.sln /p:Configuration=Release /p:Platform=x64",
    "build86": " msbuild .\\InteractiveNotifications.sln /p:Configuration=Release /p:Platform=x86",
    "build": "npm run build64 && npm run build86"
  },
  "repository": "https://github.com/felixrieseberg/InteractiveNotifications",
  "license": "MIT",
  "dependencies": {
    "bindings": "^1.2.1",
    "nan": "^2.4.0"
  },
  "os": [
    "win32"
  ],
  "author": {
    "email": "felix@felixrieseberg.com",
    "name": "Felix Rieseberg",
    "url": "http://www.felixrieseberg.com"
  },
  "devDependencies": {
    "fs-extra": "^1.0.0"
  }
}
